#include <stdio.h>

int main() {

    int h, m, s;

    // Input from user

    scanf("%d %d %d", &h, &m, &s);

    // Convert extra seconds into minutes

    m += s / 60;

    s = s % 60;

    // Convert extra minutes into hours

    h += m / 60;

    m = m % 60;

    // Output the results

    printf("Total Number of hours is %d\n", h);

    printf("Total Number of minutes is %d\n", m);

    printf("Total Number of seconds is %d\n", s);

    return 0;

}
